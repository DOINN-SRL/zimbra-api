{
    "name": "zimbra-api/soap-api",
    "type": "library",
    "description": "PHP wrapper library around the Zimbra Soap API (web service)",
    "keywords": ["library", "soap", "web service", "client", "Zimbra API"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nguyen Van Nguyen",
            "email": "nguyennv1981@gmail.com",
            "homepage": "https://github.com/nguyennv"
        }
    ],
    "require": {
        "php": ">=7.0",
        "evenement/evenement": "^3.0",
        "guzzlehttp/guzzle": "^6.3",
        "phpcollection/phpcollection": "^0.5",
        "jms/serializer": "^1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Zimbra\\": "src/Zimbra"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
