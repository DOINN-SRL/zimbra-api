{
    "name": "Zimbra/API",
    "type": "library",
    "description": "Zimbra API is a PHP soap client library for zimbra soap api (web service)",
    "keywords": ["library", "soap", "web service", "client", "Zimbra API"],
    "license": "BSD (3-Clause) License",
    "authors": [
        {
            "name": "Nguyen Van Nguyen",
            "email": "nguyennv1981@gmail.com",
            "homepage": "https://github.com/nguyennv"
        }
    ],
    "require": {
        "guzzle/http": "3.*",
        "guzzle/plugin-cookie": "3.*",
        "phpcollection/phpcollection": "0.*",
        "ext-soap": "*"
    },
    "require-dev": {
        "zendframework/zend-soap": "2.2.*",
        "react/http": "0.3.*"
    },
    "autoload": {
        "psr-0": {
            "Zimbra\\Tests": "tests/",
            "Zimbra": "src/"
        }
    }
}
